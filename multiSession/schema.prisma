// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Session {
  id          Int      @id @default(autoincrement())
  clientId    Int      @unique
  qrCodeData  String
  createdAt   Int      @default(auto()) @updatedAt
}
